{
  "name": "elegance-js",
  "version": "1.0.0",
  "description": "Web-Framework",
  "type": "module",
  "scripts": {
    "build": "esbuild --format='esm' --platform='node' --bundle --outdir='./dist' --minify --platform='node' --external:esbuild --out-extension:.js=.mjs ./src/**/*.ts && tsc",
    "prepare": "npm run build"
  },
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "author": "vqldemar",
  "license": "MIT",
  "dependencies": {
    "esbuild": "0.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.9.1"
  }
}
